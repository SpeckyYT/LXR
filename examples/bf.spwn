import "../src"

let lexer = @lxr::new([
    ["+", "PLUS"],
    ["-", "MINUS"],
    [">", "FORWARD"],
    ["<", "BACKWARD"],
    [".", "DOT"],
    [",", "COMMA"],
    ["[", "OPEN"],
    ["]", "CLOSE"],
    [regex(r"."), "ignore"],
])

output = lexer.lex(u"
    >>+++++++++++++++[-<+++++++++++++++[-<+++++++++++++++>]>]<++++++++++[-<+++++++++++>]<+.
    >++++[->++++++++<]>.[->+++<]>+.++++++++++++.++.--------.++++++++++++++.--.
")
$.print(output)
